{
    "logger": {
        "path": "./photo_upload_app.log",
        "level": "info",
        "rotation": "20 days",
        "retention": "1 months",
        "format": "[<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> {process}] <cyan>{name}</cyan> - <level>{level: <8}</level> - <level>{message}</level> | {file.path}:{line}{exception}"
    }
}